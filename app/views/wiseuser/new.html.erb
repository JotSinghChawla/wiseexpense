<h1>Wiseuser Sign Up</h1>
  <%= form_with model: @new_wiseuser, url: wiseuser_create_path, local: true do |f| %>
      
    <%= f.label "Firstname: " %>
    <%= f.text_field :first_name, required: true %>
    <br /><br />
    
    <%= f.label "Lastname: "%>
    <%= f.text_field :last_name, required: true %>
    <br /><br />
   

    <%= f.label "Email: "%>
    <%= f.email_field :email, required: true %>
    <br /><br />

    <%= f.label "Password: "%>
    <%= f.password_field :password, required: true %>
    <br /><br />

    <%= f.submit 'Sign Up' %>

  <% end %>